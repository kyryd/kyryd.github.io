    "version": "0.2.0",
    "configurations": [
{
            "name": "Debug on Web",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
                "--web-renderer",
                "html"
            ],
        },
        {
            "name": "Debug",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Debug (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Build&Run (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}